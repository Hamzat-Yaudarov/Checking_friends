{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "api/webhook.js"
    }
  ],
  "env": {
    "TELEGRAM_TOKEN": "@TELEGRAM_TOKEN",
    "NEON_CONNECTION_STRING": "@NEON_CONNECTION_STRING"
  }
}
